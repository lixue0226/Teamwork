DROP TABLE IF EXISTS `staff_evaluation`;
CREATE TABLE `staff_evaluation` (
  `SEva_ID` int(11) NOT NULL AUTO_INCREMENT,
  `Sta_ID` int(11) NOT NULL COMMENT '人员编号',
  `SIsc_type` int(11) DEFAULT NULL COMMENT '人员种类，考核对象为各个单位的市管干部。分3大类，第一类是单位主要负责人（县区为党政正职）；第二类是其他市管干部；第三类是2017年以后各县区纪委副书记。',
  `Dep_ID` int(11) DEFAULT NULL COMMENT '单位编号',
  `SEva_democracy` int(11) DEFAULT '0' COMMENT '民主测评结果,其值为民主测评结果中的编号',
  `SEva_comment` int(11) DEFAULT '1' COMMENT '政策评价能达到的最大层次，默认为1：优秀',
  `SEva_is_recommend` int(11) DEFAULT '1' COMMENT '是否推荐为优秀',
  `SEva_adjust` int(11) DEFAULT '0' COMMENT '调整后的情况',
  `SEva_final` int(11) DEFAULT '0' COMMENT '最终的评审结果',
  `SEva_year` int(11) DEFAULT NULL,
  PRIMARY KEY (`SEva_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=30 DEFAULT CHARSET=utf8 COMMENT='测评表，将领导测评和普通员工测评整合在了一起，通过staff_ischarge表来判断';

