DROP TABLE IF EXISTS `staff`;
CREATE TABLE `staff` (
  `Sta_ID` int(11) NOT NULL AUTO_INCREMENT,
  `Sta_Name` varchar(10) NOT NULL COMMENT '姓名',
  `Sta_cardID` varchar(18) NOT NULL DEFAULT '0' COMMENT '身份证号',
  `Sta_sex` int(1) DEFAULT '0' COMMENT '性别,0未设置，1为男，2为女',
  `Sta_born` datetime DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP COMMENT '出生日期',
  `Sta_nationality` varchar(10) DEFAULT '' COMMENT '民族',
  `Sta_origin` varchar(10) DEFAULT '' COMMENT '籍贯',
  `Sta_place` varchar(50) DEFAULT '' COMMENT '出生地',
  `Sta_join_party` date DEFAULT NULL COMMENT '入党时间',
  `Sta_join_job` date DEFAULT NULL COMMENT '参加工作时间',
  PRIMARY KEY (`Sta_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=10000004 DEFAULT CHARSET=utf8;

