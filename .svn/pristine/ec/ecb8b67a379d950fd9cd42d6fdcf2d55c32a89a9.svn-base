  `DCom_year` int(11) DEFAULT NULL,
  PRIMARY KEY (`DCom_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=296 DEFAULT CHARSET=utf8 COMMENT='单位备注信息，用于显示处罚，用于政策评价后的最高层次';

