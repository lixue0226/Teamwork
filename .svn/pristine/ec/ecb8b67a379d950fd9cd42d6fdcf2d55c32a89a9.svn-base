  `Sact_picture` varchar(100) DEFAULT NULL COMMENT '活动照片',
  `Sact_video` varchar(100) DEFAULT NULL COMMENT '活动录像',
  `Sact_state` int(11) DEFAULT NULL COMMENT '用于确定是否已经不能修改，0，可以修改，1，不能修改',
