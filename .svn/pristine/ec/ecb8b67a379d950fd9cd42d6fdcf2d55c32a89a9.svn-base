-- Table structure for staff_resume
-- ----------------------------
DROP TABLE IF EXISTS `staff_resume`;
CREATE TABLE `staff_resume` (
  `SRes_ID` int(11) NOT NULL AUTO_INCREMENT,
  `Sta_ID` int(11) NOT NULL COMMENT '人员编号',
  `Sta_current_duty` int(11) NOT NULL COMMENT '是否为负责人,默认0:不是负责人，1位党，2为政治，3为部门负责人，4为纪检委副书记，其中每个单位的1,2,3，4只能有1个',
  `SRes_health` varchar(10) DEFAULT NULL COMMENT '健康状况',
  `SRes_technology_duty` varchar(10) DEFAULT NULL COMMENT '专业技术职务',
  `SRes_technology_major` varchar(10) DEFAULT NULL COMMENT '熟悉专业有何专长',
  `SRes_full_education` varchar(10) DEFAULT NULL COMMENT '全日制教育',
  `SRes_full_major` varchar(10) DEFAULT NULL COMMENT '全日制教育毕业院校系及专业',
  `SRes_part_education` varchar(10) DEFAULT NULL COMMENT '在职教育',
  `SRes_part_major` varchar(10) DEFAULT NULL COMMENT '在职教育毕业院校系及专业',
  `Dep_ID` int(11) DEFAULT NULL COMMENT '拟任职务',
  `SRes_assignment_will` varchar(10) DEFAULT NULL COMMENT '拟任职务，这个在以后会作为当前的职务',
  `SRes_assignment_now` varchar(10) DEFAULT NULL COMMENT '现任职务',
  `SRes_assignment_old` varchar(10) DEFAULT NULL COMMENT '拟免职务',
  `SRes_Reasons_appointment_removal` varchar(200) DEFAULT NULL COMMENT '任免理由',
  `SRes_report_DepID` int(11) DEFAULT NULL COMMENT '呈报单位编号',
  `SRes_report_comment` varchar(200) DEFAULT NULL COMMENT '呈报单位意见',
